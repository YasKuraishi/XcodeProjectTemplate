FRAMEWORK_SEARCH_PATHS = $(DEVELOPER_DIR)/Platforms/iPhoneSimulator.platform/Developer/Library/Frameworks $(inherited) "$(SDKROOT)/Developer/Library/Frameworks" $(inherited) "$(SDKROOT)/Developer/Library/Frameworks" "$(DEVELOPER_LIBRARY_DIR)/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/AFNetworking" "${PODS_ROOT}/Headers/CRToast" "${PODS_ROOT}/Headers/Expecta" "${PODS_ROOT}/Headers/KIF" "${PODS_ROOT}/Headers/Mantle" "${PODS_ROOT}/Headers/ReactiveCocoa" "${PODS_ROOT}/Headers/ReactiveCocoa/ReactiveCocoa" "${PODS_ROOT}/Headers/ReactiveViewModel" "${PODS_ROOT}/Headers/Sculptor" "${PODS_ROOT}/Headers/Specta" "${PODS_ROOT}/Headers/iVersion" "${PODS_ROOT}/Headers/libextobjc"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/AFNetworking" -isystem "${PODS_ROOT}/Headers/CRToast" -isystem "${PODS_ROOT}/Headers/Expecta" -isystem "${PODS_ROOT}/Headers/KIF" -isystem "${PODS_ROOT}/Headers/Mantle" -isystem "${PODS_ROOT}/Headers/ReactiveCocoa" -isystem "${PODS_ROOT}/Headers/ReactiveCocoa/ReactiveCocoa" -isystem "${PODS_ROOT}/Headers/ReactiveViewModel" -isystem "${PODS_ROOT}/Headers/Sculptor" -isystem "${PODS_ROOT}/Headers/Specta" -isystem "${PODS_ROOT}/Headers/iVersion" -isystem "${PODS_ROOT}/Headers/libextobjc" -DKIF_XCTEST
OTHER_LDFLAGS = -ObjC -framework CoreGraphics -framework Foundation -framework XCTest
PODS_ROOT = ${SRCROOT}/../Pods